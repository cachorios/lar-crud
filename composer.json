{
    "name": "cachorios/crud-lar",
    "description": "Utilidades para crud",
    "keywords": ["laravel","jetstream","crud","abm" ,"Luis Rios", "cachorios", "cacho rios" ],
    "authors": [
        {
            "name": "cachorios",
            "email": "cachorios@gmail.com"
        }
    ],

    "require-dev": {
        "orchestra/testbench": "^8.10",
        "livewire/livewire": "^3.0"
    },

    "autoload": {
        "psr-4": {
            "Cachorios\\CrudLar\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cachorios\\CrudLar\\Tests\\": "tests/",
            "App\\": "stubs/app/"
        }
    },

    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Cachorios\\CrudLar\\CrudLarServiceProvider"
            ],
            "aliases": {
                "CrudLar": "Cachorios\\CrudLar\\Facades\\CrudLar"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true

}
